    <div class="containers">
      <% if(db.get('ldb')) { %>
      <h1 class="judul" id="u"><strong><i class="far fa-star"></i> Promoted</strong></h1>
  
    <%
     let a = db.get("ldb")
     let idnya = client.guilds.cache.get("798420832341721088").members.cache.filter(m => m._roles.includes("798422333815259137")).map(x => x.id)
     
     let approvedss = a.filter(x => !idnya.includes(x.id));
     let approveds = approvedss.filter(x => x.status === true);
     let approved = approveds.sort(function(a, b){return 0.5 - Math.random()});
     
     let promotes = a.filter(x => idnya.includes(x.id));
     let promote = promotes.sort(function(a, b){return 0.5 - Math.random()});
     
     
     let v = ["68944999431458008"]
    %>
    
      <div class="row row-cols-1 row-cols-md-3">
        <% promote.map(x => { %>
        
        <div class="col md-4">
          <br>
          <center>
            <div class="boxnya card rounded">
              <div class="card-image" style="position:relative">
                <img src="<%= client.users.cache.get(x.id).displayAvatarURL() %>" class="test circle responsive-image">
                <!--span class="badge badge-primary"><h5><i class="fas fa-angle-up"></i>1000 VOTE</h5></span>-->
              </div>
              
              <div class="card-body">
                <h3 class="asew card-title"><a>client.users.cache.get(x.id).username</a></h3>
                
                <div class="card-content white-text">
                  <h6 class="asew"><%= x.sdecs %></h6>
                  
                </div>
                
                <hr>
                
                <div class="asww">
                  
                  <span class="badge badge-pill badge-primary"><i class="fas fa-check-circle"></i> Approved</span>
                  <a href="/vote/<%= x.id %>" class="badge badge-pill badge-primary"><i class="fas fa-angle-up"></i> <%= db.get(`vote.${x.id}`) || 0 %> Votes</a>
                  <span class="badge badge-pill badge-primary"><i class="fas fa-star"></i> Promoted</span>
                  
                </div>
                
                <hr>
                
                <div class="card-action">
                  
                  <% if(v.includes(x.id)) { %>
                  <a class="btn btn-primary btn-block" href="https://cbot.me" type="button">View</a>
                  <% }else{ %>
                  
                  <a class="btn btn-primary" href="/bot/<%= x.id %>" type="button">View</a>
                  <a class="btn btn-primary" href="<%= x.invite %>" type="button">Invite</a>
                  
                  <% } %>
                  
                </div>
            </div>
              
              <div class="card-footer text-muted text-center">
                by <a href="/user/<%= x.ownerId %>"><%=  client.users.cache.get(x.ownerId).tag %></a>
              </div>
              
            </div>
          </center>
          
        </div>
        <% }) %>
      </div>
      
      </br></br>
  
  <h1 class="judul"><strong>Top Vote</strong></h1>
  
      <% 
      let list = Object.entries(db.get("vote")).sort((a, b) => b[1] - a[1]).slice(0, 5); let arr = [];
      for (var i in list) {
      arr.push(list[i]);
      };
      %>
      
  <div class="row row-cols-1 row-cols-md-3 text-white">
    
    <% arr.map((x, i) => { %>
    
    
    <div class="col mb-4">
      <br>
      <center>
        <div class="boxnya card rounded bg-primary">
          <div class="card-image">
            <img src="<%=client.users.cache.get(x[0]).displayAvatarURL()%>" class="test circle responsive-image" width="100">
          </div>
          
          <div class="card-body">
          <h3 class="asew card-title"><a><%= client.users.cache.get(x[0]).username %></a></h3>
          
          <br>
            <hr>
            
            <div class="asww">
              <span class="badge badge-pill badge-dark"><i class="fas fa-angle-up"></i> <%= x[1] %> Votes</span>
            </div>
            
            <hr>
            
            <div class="card-action">
              <a class="btn btn-block btn-dark btn-rounded" href="/bot/<%= x[0] %>">View</a>
            </div>
          </div>
        </div>
          
      </center>
    </div>
    
          <% }) %>
        </div>
  
  <br><br>
  
  
      <h1 class="judul"><strong><i class="fas fa-chart-line"></i> Random</strong></h1>
  
<div class="row row-cols-1 row-cols-md-3">
    <% approved.slice(0, 70).map(x => { %>
  
    <div class="col mb-4" style="height:80% !important;">
      <br>
      <center>
      <div class="boxnya card rounded">
        <div class="card-image">
          <img src="<%= client.users.cache.get(x.id).displayAvatarURL() %>" class="test circle responsive-image" width="100">
        </div>
        
        <div class="card-body">
          <h3 class="asew card-title"><a><%= client.users.cache.get(x.id).username %></a></h3>
            
            <div class="card-content white-text">
              <h6 class="asew"><%= x.sdecs %></h6>
              
            </div>
          
          <hr>
          <div class="asww">
            <span class="badge badge-pill badge-primary"><i class="fas fa-check-circle"></i> Approved</span>
            <span class="badge badge-pill badge-primary"><i class="fas fa-angle-up"></i> <%= db.get(`vote.${x.id}`) || 0 %> Votes</span>
          </div>
          <hr>
            
            <div class="card-action">
              <a class="btn btn-primary btn-rounded" href="/bot/<%=x.id%>" role="button">View</a>
              <a class="btn btn-primary btn-rounded" href="<%=x.invite%>" role="button">Invite</a>
            </div>
        </div>
        
        <div class="card-footer text-muted text-center">
          by <a href="/user/<%=x.ownerId%>"><%= client.users.cache.get(x.ownerId).tag %></a>
        </div>
        
      </div>
        </center>
  </div>
   <% }) %>
</div>
</div>
</div>

<% }else{ %>
<h1>
  Hello
</h1>
<% } %>

