<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js" integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV" crossorigin="anonymous"></script>

</body>
</html>
<style>
  
 body{
    background:url("https://cdn.glitch.com/fafda016-78f3-4788-ba74-9281fad6bc3f%2Fimages.jpeg?v=1596425116755");
  }

   .test{
    width:100px !important;
    border-radius:150px !important;
    padding:10px !important;
  }
  
  /*  KHUSUS BADGE  */
  .asww{
  text-align:left!important;  
  }
  
  
    
  .boxnya{
    width:auto;
    margin:10px;
    background:transparent;
    border:1px solid white;
    box-shadow:1px 5px 10px black !important;
    transition:1s;
  }
  
  .boxnya:hover{
    width:auto;
    margin:10px;
    background:transparent;
    border:1px solid white;
    box-shadow:2px 2px 5px white !important;
    transition:1s;
  }
  
  .badge{
    width:auto !important;
    height:20px;
    margin:2px;
  }
  
  .badge span{
    font-size:15px;
  }
  
  .asw{
    margin:4px !important;
  }
  
  /*  BADGE USER  */
  .lambang{
    width:auto !important;
    height:30px;
    background:white;
  }
  
  .userBadge{
    text-align:center;
  }
  
  h1, h5, h3{
    text-align:center;
    color:black;
    margin:30px;
  }
  
  .a{
    color:black;
    font-family: 'Chilanka', cursive !important;
  }
  
  .vote {
    border:1px solid grey;
    margin:5px;
    tex-decoration:none;
    border-radius:5px;
    background:grey;
    color:black;
    text-align:center;
    box-shadow:1px 1px 0px white;
    transition:0.8s;
  }
  
   .vote:hover {
    border:1px solid white;
    margin:5px;
    color:white;
    background:black;
    text-decoration:none;
    border-radius:2px;
