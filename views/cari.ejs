    <div class="container-fluid mb-5">
      <% if(allData.map(x => x.data).filter(x => x.status == true && client.users.cache.get(x.id).username.toLowerCase().includes(req.params.name.toLowerCase())).map(x => x).length) { %>
      <% allData.map(x => x.data).filter(x => x.status == true && client.users.cache.get(x.id).username.toLowerCase().includes(req.params.name.toLowerCase())).map(x => { %>
      <h1 class="text-center mt-5 mb-3">Random Bot</h1>
    <div class="row row-cols-1 row-cols-md-4 g-4">
      <!--allData.map(x => x.data).filter(x => x.status == true && client.users.cache.get(x.id).username.toLowerCase().includes(req.params.name.toLowerCase())).map(x => { -->
      <div class="col">
        <div class="card <% if(x.bgURL) { %>
                    custom
                    <% } %>" style="background:url(<%- x.bgURL %>);">
          <img src="<%- client.users.cache.get(x.id).displayAvatarURL({format:"png", size:2048}) %>" class="card-img-custom rounded" alt="...">
          <div class="card-body">
            <div class="btn-card mb-3">
              <a class="btn-vote" href="/vote/<%- x.id %>"><i class="fas fa-chevron-up"></i> <%- x.vote.toLocaleString() || "N/A"%></a>
            </div>
            <h5 class="card-title"><%- client.users.cache.get(x.id).username %></h5>
            <p class="card-text shortDesc"><%- x.sdecs %></p>
            <% if(x.category.map !== undefined) { %>
            <p><%- x.category.map(x => `
            <span class="badge badge-primary">${x}</span>
            `).join("") %></p>
            <% }else{ %>
            <p><span class="badge badge-primary"><%- x.category %></span></p>
            <% } %>
            <hr>
            <!--iv class="button-view-invite">
            <a class="view-invite">View</a>
            <a class="view-invite">Invite</a>
            </div-->
            <div class="row">
              <div class="col">
                <a class="view-invite" href="/bot/<%- x.id %>">View</a>
              </div>
              <div class="col">
                <a class="view-invite" href="<%- x.invite %>">Invite</a>
              </div>
            </div>
          </div>
        </div>
      </div>
      </div>
      <% })} else { %>
      <h1 class="text-center mt-5 mb-3">Bot Not Found</h1>
      <% }%>
    </div>
    
    <div class="container" data-bs-spy="" data-bs-target="#category" data-bs-offset="0" tabindex="0" style="outline:none;">
      <h1 class="" id="explore-tags">All Tags</h1>
      <div class="all-category" style="background-color:#2c2f33;">
        <% category.map(x => { %>
        <a class="category btn" href="/tag/<%- x.name.toLowerCase() %>"><i class="<%- x.icon %>"></i> <%- x.name %></a>
        <% }) %>
      </div>
    </div>
    <br><br>
    
  </body>
</html>
